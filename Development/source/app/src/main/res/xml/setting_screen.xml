<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:sample="http://schemas.android.com/apk/res-auto"
    android:key="setting_pre">
    <SwitchPreference
        android:title="@string/you_age"
        android:key="switch_age"
        android:icon="@mipmap/age"
        android:summaryOff="Off"
        android:summaryOn="On"
        android:background="@drawable/customshape"
        android:switchTextAppearance="@style/TextColor"
    >
    </SwitchPreference>
    <SwitchPreference
        android:title="@string/gender_setting"
        android:icon="@mipmap/gender"
        android:key="switch_gender"
        android:textColor="@color/blackColor"
        android:summaryOff="Off"
        android:summaryOn="On">
    </SwitchPreference>
    <SwitchPreference
        android:title="@string/emotion_setting"
        android:key="switch_emotion"
        android:icon="@mipmap/emotion"
        android:summaryOff="Off"
        android:switchTextAppearance="@color/blackColor"
        android:textColor="@color/blackColor"
        android:summaryOn="On">
    </SwitchPreference>
    <SwitchPreference
        android:title="@string/characteristic_setting"
        android:key="switch_character"
        android:icon="@mipmap/characteristic"
        android:summaryOff="Off"
        android:textColor="@color/blackColor"
        android:summaryOn="On">
    </SwitchPreference>
    <com.pavelsikun.seekbarpreference.SeekBarPreference
        android:key="amount_face"
        android:title="@string/amount_setting"
        android:textColor="@color/blackColor"
        android:summary="@string/description_amount"
        android:textColorHighlight="@color/blackColor"
        sample:msbp_maxValue="10"
        />
</PreferenceScreen>